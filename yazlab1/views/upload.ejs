        <!-- Start: Cart Checkout Section -->
        <div id="content" class="site-content">
            <div id="primary" class="content-area">
                <main id="main" class="site-main">
                    <div class="checkout-main">
                        <div class="container">
                          <div style="visibility: hidden; "id="errmsg" class="alert alert-danger" role="alert">
                  
                          </div>
                          <div style="visibility: hidden; "id="ok" class="alert alert-success" role="alert">
                  
                          </div>
                            <div class="row">
                                <div class="cart-head">
                                   
                                <div class="col-md-12">
                                    <article class="page type-page status-publish hentry">
                                        <div class="entry-content">
                                            <div class="woocommerce">

                                                <form action="/upload" class="checkout woocommerce-checkout" method="post" id="formgonder" >
                                                    
                                                    <div class="row">
                                                        <div id="customer_details">
                                                            <div class="col-xs-12 col-sm-6">
                                                                <div class="woocommerce-billing-fields">
                                                                    <h2>Kitap  Bilgileri</h2>
                                                                    <span class="underline left"></span>
                                                                    <div class="row">
                                                                        <div class="billing-address-box">
                                                                          <div class="clear"></div>
                                                                          <div id="autoisbnmode">
                                                                            <div class="col-xs-12 col-sm-6">
                                                                                <p id="billing_first_name_field" class="form-row form-row form-row-first">
                                                                                    <input type="text" value=""  placeholder="Kitap Adı" id="kitap_name" name="kitap_name" class="input-text">
                                                                                </p>
                                                                            </div>
                                                                            <div class="col-xs-12 col-sm-6">
                                                                                <p id="billing_last_name_field" class="form-row form-row form-row-last validate-required">                                                                        
                                                                                    <input type="text" value="" autocomplete="family-name" placeholder="Kitap Yazarı" id="kitap_author" name="kitap_author" class="input-text">
                                                                                </p>
                                                                            </div>
                                                                          </div>
                                                                            <div class="clear"></div>
                                                                            <div class="col-xs-12 col-sm-6">
                                                                                <p id="billing_company_field" class="form-row form-row form-row-wide">
                                                                                    <label for="kitap_kapak">Kitap Kapagi</label>
                                                                                    <img hidden id="kapaksrc" alt="No Image" />
                                                                                    <input type="file" value="" autocomplete="organization" id="kitap_kapak"  class="input-file ">
                                                                                    <input id="frontimage" type="hidden" name="kitap_imagefront" value=""/>
                                                                                    <input id="isbnimage" type="hidden" name="kitap_imageisbn" value=""/>

                                                                                </p>
                                                                            </div>
                                                                            <div class="col-xs-12 col-sm-6">
                                                                                <p id="billing_company_field" class="form-row form-row form-row-wide">
                                                                                    <label for="kitap_isbn">Kitap isbn</label>
                                                                                    <img hidden id="imageSrc" alt="No Image" />
                                                                                    <input type="file" value="" autocomplete="organization" id="kitap_isbn" class="input-file ">
                                                                                </p>
                                                                            </div>
                                                                            <label  class="col-xs-12 col-sm-6 " for="canvasOutput">ISBN FOTO</label>
                                                                            <canvas class="col-xs-12 col-sm-6 "id="canvasOutput" ></canvas>
                                                                            <label class="col-xs-12 col-sm-6 " for="canvaskapak">Kitap Kapağı</label>
                                                                            <canvas class="col-xs-12 col-sm-6 "id="canvaskapak" ></canvas>
                                                                            <div class="clear"></div>
                                                                            <div class="col-xs-12 col-sm-6">
                                                                                <p id="billing_last_name_field" class="form-row form-row form-row-last validate-required">                                                                        
                                                                                    <button id="gonderbutton" type="button"> gonder </button>
                                                                                    </p>
                                                                            </div>
                                                                        </div>  
                                                                    </div>
                                                                    <div class="clear"></div>
                                                                </div>
