        <% if(error==1){ %> 
          <script>
            $(document).ready(function(){
             document.getElementById("errmsg").style.visibility="visible";
             document.getElementById("errmsg").innerHTML = "Var olan Kitap yüklenemez..."
              });
          </script>
          <%} else if(error==2){ %> 
            <script>
              $(document).ready(function(){
               document.getElementById("errmsg").style.visibility="visible";
               document.getElementById("errmsg").innerHTML = "Bilgiler Eksik..."
                });
            </script>
            <%} else if(error==3){ %>
              <script>
                $(document).ready(function(){
                 document.getElementById("errmsg").style.visibility="visible";
                 document.getElementById("errmsg").innerHTML = "ISBN okunamadı..."
                  });
              </script>
          <% } else if(ok==1){ %> 
            <script>
              $(document).ready(function(){
               document.getElementById("ok").style.visibility="visible";
               document.getElementById("ok").innerHTML = "Yükleme Başarılı"
                });
            </script>
            <% } %> 
