                    <div class="entry-content">
                    
                      <div class="woocommerce table-tabs" id="responsiveTabs">
                       
                        <ul class="nav nav-tabs">
                          <% for( let z = 0; z < kitaplar.length; z++ ) { %>
                          <li >
                            <b class="arrow-up"></b><a data-toggle="tab" href="#section<%=kitaplar[z][0]["user_username"]%>"><%=kitaplar[z][0]["user_username"]%></a>
                          </li>
                          <% } %>
                          
                        </ul>
                        
                      <div class="tab-content">
                         <% for( let i = 0; i < kitaplar.length; i++ ) { %>
                         <div id="section<%=kitaplar[i][0]["user_username"]%>" class="tab-pane fade in <% if (i==0) { %>
                        active
                         <% } %> ">
                            <form method="post" action="http://libraria.demo.presstigers.com/cart-page.html">
                              <table class="table table-bordered shop_table cart">
                                <thead>
                                  <tr>
                                    <th class="product-name">&nbsp;</th>
                                    <th class="product-name">Kitap</th>
                                    <th class="product-quantity">Alınma Tarihi</th>
                                    <th class="product-price">Teslim Tarihi </th>
                                      
                                    <th class="product-subtotal">&nbsp;</th>
                                  </tr>
                                </thead>
                    
                                <% for( let j = 0; (j < kitaplar[i].length) &&  Object.keys(kitaplar[i][j]).length >1;
                                                          j++ ) { %>
                                <tbody>
                                  <tr class="cart_item">
                                    <td data-title="cbox" class="product-cbox">
                                      <span>
                                        <input type="checkbox" id="cbox3" value="first_checkbox" />
                                      </span>
                                    </td>
                                    <td data-title="Product" class="product-name">
                                      <span class="product-thumbnail">
                                        <a href="#"><img src="<%=kitaplar[i][j]["book_image"]?`data:image/png;base64,${kitaplar[i][j]["book_image"]}`:"/images/icon-magazine.png"%>" alt="cart-product-1" 
                                          height="111" width="110"/></a>
                                      </span>
                                      <span class="product-detail">
                                       
                                        <h5 class="card-title">
                                          <%=kitaplar[i][j]["book_name"]%></h5>
                                        
                                      

                                      <span
                                        ><strong>Yazar:</strong>
                                        <%=kitaplar[i][j]["book_author"]%></span>
                                      
                                      
                                      <span
                                        ><strong>ISBN:</strong> <%=
                                        kitaplar[i][j]["book_isbn"]%></span >
                                     
                                    
                                    </span>
                                    </td>
                          
                                    <td data-title="Price" class="product-price">
                                      <p>
                                        <%  let a = new Date( kitaplar[i][j]["sale_date"]);
                                        a.setDate( a.getDate() - 7 );
                                        %> 
                                        <%=a.getDate()%>/<%=a.getMonth()+1%>/<%=a.getFullYear()%><br />
                                        <%=a.getHours()%>:<%=a.getMinutes()%>:<%=a.getSeconds()%>

                                      </p>
                                    </td>
                                    <td class="product-remove">
                                      <p>
                                        <% let b = new Date( kitaplar[i][j]["sale_date"]); %> 
                                       
                                        <%=b.getDate()%>/<%=b.getMonth()+1%>/<%=b.getFullYear()%><br />
                                        <%=b.getHours()%>:<%=b.getMinutes()%>:<%=b.getSeconds()%><br />
                                        <% 
                                        const diffTime =   b - servertime;
                                        const diffDays = Math.ceil( diffTime / (1000 * 60 * 60 * 24));  %> 
                                        Kalan gün: <%if (diffDays <=0){ %> Süre bitti.  <%} else {  %> <%=diffDays%> gün kaldı <% } %>  
                                     
                                      </p>
                                    </td>
                                  </tr>
                                </tbody>
                          
                                <% } %> 
                              </table>
                            </form>
